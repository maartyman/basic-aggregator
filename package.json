{
  "name": "solid-aggregator",
  "version": "0.0.0",
  "description": "A universal implementation of a solid-aggregator.",
  "license": "MIT",
  "homepage": "https://github.com/maartyman/solid-aggregator#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/maartyman/solid-aggregator.git"
  },
  "bugs": {
    "url": "https://github.com/maartyman/solid-aggregator/issues"
  },
  "keywords": [
    "solid",
    "aggregator"
  ],
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "bin": {
    "solid-aggregator": "bin/server.js"
  },
  "files": [
    "bin",
    "config",
    "dist"
  ],
  "engines": {
    "node": ">=18.0"
  },
  "lsd:module": true,
  "scripts": {
    "build": "npm run build:ts && npm run build:components",
    "build:ts": "tsc",
    "build:components": "componentsjs-generator -i .componentsignore",
    "prepare": "npm run build",
    "jest": "jest --coverageReporters text-summary --",
    "lint": "eslint . --cache --max-warnings 0",
    "lint:fix": "eslint . --max-warnings 0 --fix",
    "start": "node ./bin/server.js",
    "test": "npm run jest"
  },
  "dependencies": {
    "componentsjs": "^5.4.2",
    "tinyqueue": "^2.0.3"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.4.5",
    "@tsconfig/node18": "^18.2.2",
    "@types/jest": "^29.5.5",
    "componentsjs-generator": "^3.1.2",
    "eslint-plugin-jest": "^27.6.0",
    "husky": "^4.3.8",
    "jest": "^29.7.0",
    "jest-rdf": "^1.8.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "5.1.6"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run build && npm run lint && npm run test"
    }
  }
}
